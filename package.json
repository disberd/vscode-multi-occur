{
  "name": "occur",
  "displayName": "Occur",
  "description": "Add Cursors to occurences of target text in selected portions of a document",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "occur.RemoveSelections",
        "title": "Occur: Remove Selections"
      },
      {
        "command": "occur.ToggleOccurence",
        "title": "Occur: Toggle Occurrence"
      },
      {
        "command": "occur.NextOccurence",
        "title": "Occur: Next Occurrence"
      },
      {
        "command": "occur.RemoveOccurences",
        "title": "Occur: Remove Occurrences"
      },
      {
        "command": "occur.CreateCursors",
        "title": "Occur: Create Cursors"
      },
      {
        "command": "occur.FindOccurences",
        "title": "Occur: Find Occurences"
      },
      {
        "command": "occur.AddSelection",
        "title": "Occur: Add Selections"
      }
    ],
    "keybindings": [
      {
        "key": "Escape",
        "command": "occur.RemoveSelections",
        "when": "!editorHasMultipleSelections && occur.active_region && !occur.active_occur && vim.mode == Normal"
      },
      {
        "key": "Escape",
        "command": "occur.RemoveOccurences",
        "when": "!editorHasMultipleSelections && occur.active_occur && vim.mode == Normal"
      },
      {
        "key": "Tab",
        "command": "occur.ToggleOccurence",
        "when": "occur.active_occur && vim.mode == Normal"
      },
      {
        "key": "n",
        "command": "occur.NextOccurence",
        "when": "occur.active_occur && vim.mode == Normal"
      },
      {
        "key": "Enter",
        "command": "occur.CreateCursors",
        "when": "occur.active_occur && vim.mode == Normal"
      },
      {
        "key": "Enter",
        "command": "occur.FindOccurences",
        "when": "vim.mode == Visual"
      },
      {
        "key": "Shift+Space",
        "command": "occur.AddSelection",
        "when": "vim.mode == Visual || vim.mode == VisualLine"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.45.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
